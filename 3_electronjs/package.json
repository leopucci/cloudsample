{
  "name": "electron-lessons",
  "version": "1.0.1",
  "description": "A sample Electron (JS) project for Medium lessons.",
  "main": "app/index.js",
  "scripts": {
    "start": "electron .",
    "sass:build": "node-sass app/scss -o app/css",
    "sass:watch": "node-sass -w app/render/scss -o app/render/css",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder -mwl --publish never",
    "publish": "electron-builder -mwl --publish always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/course-one/electron-lessons.git"
  },
  "keywords": [],
  "author": {
    "name": "Uday Hiwarale",
    "email": "thatisuday@gmail.com",
    "url": "https://thatisuday.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/course-one/electron-lessons/issues"
  },
  "homepage": "https://github.com/course-one/electron-lessons#readme",
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "electron-updater": "^4.3.5",
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "chokidar": "^3.4.3",
    "drag-drop": "^6.1.0",
    "fs-extra": "^9.0.1",
    "open": "^7.3.0"
  }
}
