{
  "name": "pocketCloud",
  "version": "1.0.0",
  "description": "Pocket Cloud client for the cloud",
  "main": "app/index.js",
  "scripts": {
    "start": "npm run build && electron .",
    "build": "tsc",
    "sass:build": "node-sass app/scss -o app/css",
    "sass:watch": "node-sass -w app/render/scss -o app/render/css",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder -mwl --publish never",
    "publish": "electron-builder -mwl --publish always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/course-one/electron-lessons.git"
  },
  "keywords": [],
  "author": {
    "name": "Pocket Cloud",
    "email": "pocket@pocket.com",
    "url": "https://site.com"
  },
  "license": "Private",
  "bugs": {
    "url": "https://github.com/course-one/electron-lessons/issues"
  },
  "homepage": "https://github.com/course-one/electron-lessons#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "electron-builder": "^22.11.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "ts-migrate": "^0.1.26",
    "tsc": "^2.0.3"
  },
  "dependencies": {
    "@felipebutcher/node-os-info": "^0.0.5",
    "@sentry/electron": "^2.5.0",
    "better-sqlite3-sqleet": "^7.1.2",
    "chokidar": "^3.5.1",
    "drag-drop": "^6.1.0",
    "electron": "^12.0.7",
    "electron-is-dev": "^2.0.0",
    "electron-positioner": "^4.1.0",
    "electron-updater": "^4.3.8",
    "fs-extra": "^9.1.0",
    "loadavg-windows": "^1.1.1",
    "mqtt": "^4.2.6",
    "node-machine-id": "^1.1.12",
    "open": "^7.4.2",
    "typescript": "^4.4.4",
    "uuid": "^3.4.0",
    "winston": "^3.3.3",
    "workerpool": "^6.1.4"
  }
}
